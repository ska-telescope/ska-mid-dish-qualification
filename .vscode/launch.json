    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "DiSQ GUI",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/disq/mvcmain.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "DISQ_CONFIG": "${workspaceFolder}/disq.ini"
            }
        },
    ]
}